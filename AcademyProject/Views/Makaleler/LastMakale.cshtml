
@model IEnumerable<EntityLayer.Concrete.Makale>

<section id="services" class="services">
	<div class="container">

		<div class="row">
			@foreach (var item in Model)
			{
				<div class="col-lg-4 col-md-6">
					<div class="icon-box" data-aos="fade-up">
						<h4 class="title"><a href="">@item.MakaleTitle</a></h4>
						<p class="description">@item.MakaleDesc</p>
					</div>
				</div>
			}
		</div>

	</div>
</section>
