
@model IEnumerable<EntityLayer.Concrete.MesKurUye>

@{
	ViewBag.Title = "Index";
	Layout = "~/Views/Shared/_AnaLayout.cshtml";
}

<section id="breadcrumbs" class="breadcrumbs">
	<div class="container">

		<div class="d-flex justify-content-between align-items-center">
			<h2>Meslek Kuruluşlara Üyelikler</h2>
			<ol>
				<li><a href="/Ana/Index/">Ana Sayfa</a></li>
				<li>Meslek Kuruluşlara Üyelikler</li>
			</ol>
		</div>

	</div>
</section><!-- End Breadcrumbs -->
<!-- ======= Services Section ======= -->
<section id="services" class="services">
	<div class="container">

		<div class="row">
			@foreach (var item in Model)
			{
				<div class="col-lg-6 col-md-6">
					<div class="icon-box" data-aos="fade-up">
						<h4 class="title"><a href="">@Html.Raw(item.Title)</a></h4>
					</div>
				</div>
			}
		</div>

	</div>
</section><!-- End Services Section -->
